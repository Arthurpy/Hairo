{
    "version": 2,
    "builds": [
      { "src": "/back/Hairo_Back", "use": "@vercel/python" },
      { "src": "./", "use": "@vercel/static-build" }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/backend/backend/\$1" },
      { "src": "/(.*)", "dest": "/frontend/dist/\$1" }
    ]
  }
  